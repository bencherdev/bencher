---
import DocsPanel from "../../../components/docs/DocsPanel.astro";
import { otherLanguages } from "../../../i18n/ui";
export async function getStaticPaths() {
	return otherLanguages.map((lang) => {
		return { params: { lang } };
	});
}
const { lang } = Astro.params;
---

<DocsPanel lang={lang}/>
