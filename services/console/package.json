{
	"name": "bencher-console",
	"type": "module",
	"version": "0.5.7",
	"scripts": {
		"fmt": "npx biome format --write .",
		"lint": "npx biome lint .",
		"knip": "knip",
		"typeshare": "typeshare --version | grep -q 'typeshare 1.13.2' && typeshare ../.. --lang=typescript --output-file=./src/types/bencher.ts",
		"wasm": "./build_wasm.sh",
		"wasm:not-plus": "IS_BENCHER_PLUS=false npm run wasm",
		"copy": "cp -r ../cli/templates/output ./public/download && cp ../api/openapi.json ./public/download/openapi.json",
		"setup": "npm run typeshare && npm run wasm && npm run copy",
		"dev": "npm run setup && export BENCHER_API_URL=http://127.0.0.1:61016 && export OAUTH_GITHUB=true && export OAUTH_GOOGLE=true && astro dev --port 3000",
		"dev:not-plus": "IS_BENCHER_PLUS=false npm run dev",
		"start": "astro dev --port 3000",
		"test": "npm run setup && vitest",
		"build": "export $(cat .env.runtime) && npm run copy && astro build",
		"adapter": "node adapter.js",
		"node": "npm ci && npm run adapter node && npm run build",
		"netlify": "npm ci && npm run adapter netlify && npm run build",
		"preview": "export BENCHER_API_URL=http://127.0.0.1:61016 && export OAUTH_GITHUB=true && OAUTH_GOOGLE=true && astro preview --port 3000",
		"astro": "astro",
		"check": "astro check",
		"upgrade": "npx @astrojs/upgrade"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/mdx": "^4.0.8",
		"@astrojs/netlify": "^6.1.0",
		"@astrojs/node": "^9.0.3",
		"@astrojs/partytown": "^2.1.3",
		"@astrojs/sitemap": "^3.2.1",
		"@astrojs/solid-js": "^5.0.4",
		"@observablehq/plot": "^0.6.16",
		"@sentry/astro": "^9.0.1",
		"@solid-primitives/resize-observer": "^2.1.0",
		"@solid-primitives/scheduled": "^1.5.0",
		"astro": "^5.2.6",
		"astro-expressive-code": "^0.40.2",
		"astro-seo": "^0.8.4",
		"axios": "^1.7.9",
		"bulma": "^1.0.3",
		"d3": "^7.9.0",
		"htl": "^0.3.1",
		"mermaid": "^11.4.1",
		"slugify": "^1.6.6",
		"solid-devtools": "^0.33.0",
		"solid-js": "^1.9.4",
		"typescript": "^5.7.3",
		"vite-plugin-wasm-pack": "^0.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/d3": "^7.4.3",
		"hast-util-from-html-isomorphic": "^2.0.0",
		"knip": "^5.44.1",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"sass": "^1.84.0",
		"vitest": "^3.0.5"
	},
	"overrides": {
		"@astrojs/solid-js": {
			"solid-devtools": "^0.33.0"
		},
		"d3": {
			"d3-scale-chromatic": "3.1.0"
		}
	}
}
